
xmllint --html --format --xpath '/html/body/div[@id="primarywrap"]/table/tr/td[@class="maincontent"]/div[@id="feature"]/div[@class="browser_outer"]/div[@class="category_listing"]/div[@class="category_games_listing hasLayout"]/div/div/div/span/span[@class="gameplays"]/text()' 1 2>/dev/null 1>3
xmllint --html --format --xpath '/html/body/div[@id="primarywrap"]/table/tr/td[@class="maincontent"]/div[@id="feature"]/div[@class="browser_outer"]/div[@class="category_listing"]/div[@class="category_games_listing hasLayout"]/div/div/div/span/span[@class="gameplays"]/text()' 2 2>/dev/null 1>4
echo $((`paste -s -d+ 3 | bc`+`paste -s -d+ 4 | bc`)) > 5
cat 5
