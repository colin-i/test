

rps=( `cat g` )
i=0

QUERY='
{"query": "
  query {
    viewer {
      repositories(first:100){
        edges{
          node{
            name
          }
        }
      }
    }
  }"
}'

#-o 1
reps=( $(
curl https://api.github.com/graphql -X POST -H "Authorization: bearer `cat ~/n/pat`" --data "$(echo $QUERY)" | \
  jq -r ".data.viewer.repositories.edges[] | .node.name"
))

for name in "${reps[@]}"; do

prevCount=`echo -n ${rps[${i}]} | grep -o [^=]*$`

./g1 ${name} ${prevCount}

#a.out a waiter with a socket that has processedCount
out="${out} ${name}=`./a.out ${prevCount}`"

i=$((i+1))

done

out=${out:1}
echo ${out}