
y=${1}
shift
m=${1}
shift

source ./newparse

cd ${y}/${m}

reps=( `cat h` )

j=${#reps[@]}
k=$((j-i))
h=0

for (( l=k; l < ${j}; l++ )); do
	IFS='=' read -a addr <<< "${reps[${l}]}"
	addr[1]=$((addr[1]+b[${h}]))
	h=$((h+1))
	reps[${l}]=" ${addr[0]}=${addr[1]}"
done

out=
for v in "${reps[@]}"; do
	out="${out} ${v}"
done
out=${out:1}
echo -n ${out} > h

echo
cat h
echo
