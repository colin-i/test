
a=( `cat ~/measures/text_last` )
b=( `cat ~/measures/text` )
n=( `cat ~/measures/text | wc -l` )
x=0
for ((i = 0; i < ${n}; i++ )); do
	if [ ${a[${i}]} != ${b[${i}]} ]; then x=$((x+1)); fi
done

y=$((n/2))
echo -n "changes: ${x} against ${n}. "
if [ ${x} -ge ${y} ]; then
	echo ${x} ge ${y}. ok.
else
	echo ${x} lt ${y}. more changes required.
	exit 1
fi
