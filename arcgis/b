
#1 is added

cd ~/measures/new${1} || exit 1
files=( `find -type f` )

#verify same date
d=`grep "Edited by cestrin" ${files[0]}`
if [ -z "${d}" ]; then exit 1; fi
n=`grep -r "${d}" | wc -l`
if [ ${n} != ${#files[@]} ]; then exit 1; fi
echo same date. ok.

for f in "${files[@]}"; do
	if [ -e ../ex${1}/${f} ]; then a=`cat ../ex${1}/${f}`
	else a="stadiu_actual_fizic "; fi
	echo -n `grep -Poe "(?<=^${a})[^ ]*" ${f}` > ../current${1}/${f}
done
