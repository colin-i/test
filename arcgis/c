#1 is added

if [ -z "${has_new}" ]; then
	python c.py ${1} || exit 1
	diff ~/measures/text_last${1} ~/measures/text${1}
	if [ $? -eq 0 ]; then
		echo ${1} no diff
		exit 0
	fi
	if [ -n "${1}" ]; then
		./n ${1}
		exit 2
	fi
	./d || exit 1
	cp ~/measures/text ~/measures/text_last
else #has_new here only at ./a
	has_new= python c.py && \
	cp ~/measures/text ~/measures/text_last && \
	python c.py
fi && \
sed -i "s/\./,/g" ~/measures/text && \
cat ~/measures/text | xclip -selection clipboard
