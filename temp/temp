
dir=`pwd`
cd ~/gree-remote/PythonCLI

if [ -z "${1}" ]; then
min=25
else
min=${1}
fi

if [ -z "${2}" ]; then
max=27
else
max=${2}
fi

if [ -z "${3}" ]; then
dif=0
else
dif=${3}
fi

if [ -z "${4}" ]; then
send=1
else
send=0
fi

if [ -z "${5}" ]; then
sen2=0
else
sen2=1
fi

if [ -z "${6}" ]; then
base=${dir}
else
base=${6}
fi

python ${dir}/temperature_sensor_code.py ${min} ${max} ${dif} ${send} ${sen2} ${base}
