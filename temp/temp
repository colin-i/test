
cd `dirname ${0}`
dir=`pwd`
cd ~/gree-remote/PythonCLI

if [ -z "${1}" ]; then
on=0
else
on=${1}
fi

if [ -z "${2}" ]; then
sen2=0
else
sen2=${2}
fi

if [ -z "${3}" ]; then
dif=0
else
dif=${3}
fi

if [ -z "${4}" ]; then
min=24
else
min=${4}
fi

if [ -z "${5}" ]; then
max=27
else
max=${5}
fi

if [ -z "${6}" ]; then
send=1
else
send=0
fi

if [ -z "${7}" ]; then
base=${dir}
else
base=${6}
fi

python ${dir}/temperature_sensor_code.py ${on} ${sen2} ${dif} ${min} ${max} ${send} ${base}
