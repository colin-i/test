set -e

msg=aur ~/test/de/msg

name=`cat debian/control | grep "^Source" | grep -o [^\ ]*$`
if [ -n "${py}" ]; then name=python-${name}; fi
echo ${name}

cd ~/pkgbuilds/${name}
verlocal="`cat ./PKGBUILD | grep ^pkgver | grep -o [^=]*$`"
rellocal="`cat ./PKGBUILD | grep ^pkgrel | grep -o [^=]*$`"

~/test/pkg/verquery ${name} ${verlocal} ${rellocal}
