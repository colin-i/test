
if [ ~/test/pyp/pyqu = 0 ]; then
	echo same python build version
	exit 0
fi
echo pypi.org \? y enter && \
read -t $((60*60*24)) ans && \
if [ "${ans}" = "y" ]; then
#version at v2 is incremented at ./pyver (then pushed)
	~/test/pyp/pypre && \
	~/test/pyp/pyin && \
	~/test/pyp/pyout && \
	~/test/pyp/pyin x && \
	~/test/pyp/pyqu && \
	echo and yank
else
	if [ -z "${exit}" ]; then
		exit 1
	else
		exit ${exit}
	fi
fi
