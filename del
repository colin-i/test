
name=`cat debian/changelog | head -1 | grep -o ^[^\ ]*`

if [ -z "${1}" ]; then
	echo test
	cd ~/test
	sudo act -j del --env file=${name} --env tag=1.5 -s PAT=`cat ~/n/pat` workflow_dispatch
else
	echo prev app delete
	if [ ! -f "${HOME}/dontdeleteprevappimage" ]; then
		echo yes
		prev=`cat debian/changelog | grep ^${name} | sed -n 2p | grep -Poe "[^\(]*(?=\))"`
		sudo act -j del --env file=${name}.amd64.AppImage --env tag=${prev} -s PAT=`cat ~/n/pat` workflow_dispatch
	else
		echo no
	fi
fi
