set -e
if [ -z "${mime}" ]; then
	ext="${1##*.}"
	if [ ${ext} = txt ]; then
		mime=text/plain
	else
		mime=application/vnd.oasis.opendocument.spreadsheet
	fi
fi

~/test/google/dif $1

f=~/Desktop/1
mimeType=${mime} folderid=`cat ~/n/pat` ~/test/google/a $@ | tee ${f} && \
{
	q=`cat ${f} | head -1`
	rm ${f}
	query_result=${q} python ~/test/google/upd.py
}
