 	if stride!=teststride
 	#this is with alpha
 		importx "_gdk_pixbuf_get_height" gdk_pixbuf_get_height
@@ -531,8 +532,8 @@ function rgb_test(sd pixbuf)
 					inc pointer;inc bytes;set pointer# bytes#
