
sudo docker pull ubuntu:latest
sudo docker container create ubuntu:latest
id=`sudo docker container ls -a -f ancestor=ubuntu:latest -q | head -1`
sudo docker cp ./script ${id}:/
sudo docker commit ${id} rufflebld:latest
sudo docker push image0/rufflebld:latest

