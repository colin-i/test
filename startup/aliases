
alias gre='grep --color=auto -r --exclude-dir={.git,.pc,patches} --exclude=\*.dbl --exclude=\*.png --exclude=\*.bmp --exclude=\*.zip --exclude=\*.mp3 --exclude=\*.swf --exclude=\*.log --exclude=\*.edorrestorefile'
alias mov='~/test/startup/mv'
alias cop='~/test/startup/cp'
alias gfind='echo "#include <gtk/gtk.h>" | cc `pkg-config --cflags gtk4` -E -dD - | grep'
