
alias gre='grep --color=auto -r --exclude-dir={.git,.pc,patches} --exclude=\*.dbl --exclude=\*.png --exclude=\*.bmp --exclude=\*.zip --exclude=\*.mp3 --exclude=\*.swf --exclude=\*.log'
alias mov='~/test/startup/mv'
alias cop='~/test/startup/cp'
alias off='source ~/test/net/off ${1}'
alias on='sudo ifconfig enp2s0 up'
