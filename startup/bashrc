
if [ -z "${choose_term_profile}" ]; then
	if [ -z "${SSH_TTY}" ]; then   #is not from ssh
	#	${SSH_CONNECTION} , ${SSH_CLIENT}
		~/test/startup/setcolors || exit
	fi
else
	echo ${choose_term_profile}
fi

if [ "${resizeterm}" != "" ]; then ~/test/startup/termsz; fi

uname -a
if [ -f /var/run/reboot-required ]; then echo 'reboot required'; fi

a=~/.local/bin
echo ${PATH} | grep -Fq "${a}"
if [ $? = 1 ]; then
	export PATH=${a}:$PATH
fi
export PATH=~/.cargo/bin:~/Desktop/act:$PATH
export PYTHONPYCACHEPREFIX=~/pycache
#for what? export PYTHONPATH=$PYTHONPATH:
export PKG_CONFIG_PATH=/usr/lib/x86_64-linux-gnu/pkgconfig:$PKG_CONFIG_PATH

echo PATH=${PATH} PYTHONPYCACHEPREFIX=${PYTHONPYCACHEPREFIX} PKG_CONFIG_PATH=${PKG_CONFIG_PATH}

source ~/test/startup/aliases

~/test/startup/bookmark
