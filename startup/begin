
window_pid=`xdotool getactivewindow`
if [ -f ~/desktops ]; then
	xdotool windowmove ${window_pid} `cat ~/desktops` 0
	wmctrl -r :ACTIVE: -b toggle,maximized_vert,maximized_horz
else
	window_pid=`xdotool getactivewindow` ~/test/startup/termsz  #if window_pid was input ( window_pid= ./begin ) it was passed
fi

~/test/act/a

~/test/bookmarks/bookmark

~/test/misc/storage

if [ -z "${is_vnc}" ]; then
	~/test/net/data/a

	if [ -z "${no_temp}" ]; then
		~/arh/activity &
		sleep 10 #to not override text with temp
		if [ -z "${3}" ]; then a=-1
		else a=${3}
		fi
		~/test/plug/a ${a} ${1} ${2}
	else
		~/arh/activity
	fi
else
	~/test/vnc/vnc
fi
